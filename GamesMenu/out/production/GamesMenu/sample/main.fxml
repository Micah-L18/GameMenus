<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="720.0" prefWidth="1270.0">
         <children>
            <TabPane layoutX="-14.0" layoutY="-14.0" prefHeight="736.0" prefWidth="1286.0" tabClosingPolicy="UNAVAILABLE">
               <tabs>
                  <Tab text="Number Guesser">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <Button fx:id="playButton" layoutX="554.0" layoutY="499.0" mnemonicParsing="false" onAction="#checkNum" prefHeight="50.0" prefWidth="163.0" text="Play" visible="false"/>
                              <TextField fx:id="userGuess" layoutX="561.0" layoutY="372.0" visible="false" />
                              <Label layoutX="619.0" layoutY="337.0" text="Guess" />
                              <Label fx:id="gameResult" layoutX="619.0" layoutY="435.0" text="Result" />
                              <Button fx:id="playAgain" layoutX="554.0" layoutY="568.0" mnemonicParsing="false" onAction="#newGame" prefHeight="51.0" prefWidth="163.0" text="Play Again!" visible="false" />
                              <Label layoutX="548.0" layoutY="244.0" text="Guess a number between 1-1000" />
                              <Button fx:id="startButton" layoutX="572.0" layoutY="129.0" mnemonicParsing="false" onAction="#startGame" prefHeight="47.0" prefWidth="127.0" text="start" />
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
                  <Tab text="Tic Tac Toe">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                     </content>
                  </Tab>
               </tabs>
            </TabPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>